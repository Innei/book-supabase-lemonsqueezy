  "scripts": {
    "prepare": "pnpm exec simple-git-hooks",
    "dev": "next dev -p 9988",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "pnpm exec drizzle-kit migrate",
    "db:push": "pnpm exec drizzle-kit push"
